FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "6";
    object      nu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.5e-05;

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform 1.5e-05;
    }
    inlet
    {
        type            calculated;
        value           uniform 1.5e-05;
    }
    outlet
    {
        type            calculated;
        value           uniform 1.5e-05;
    }
}


