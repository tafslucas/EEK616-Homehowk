FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0;
        value           nonuniform List<scalar> 0;
    }
    base
    {
        type            zeroGradient;
    }
    sides
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 300;
    }
}


