FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    outlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0;
    }
    base
    {
        type            calculated;
        value           uniform 101325;
    }
    sides
    {
        type            calculated;
        value           uniform 101325;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 101325;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 101325;
    }
}


